{
  "resourceType": "StructureDefinition",
  "id": "profile-goal",
  "url": "http://hl7.org/fhir/ca/core/StructureDefinition/profile-goal",
  "version": "0.0.0",
  "name": "GoalProfile",
  "status": "draft",
  "publisher": "TBD",
  "description": "Proposed constraints and extensions on the Goal Resource to support collection of diagnostic report information.  Generated as a first step toward creating a set of Canadian Core FHIR profiles.",
  "fhirVersion": "4.0.0",
  "kind": "resource",
  "abstract": false,
  "type": "Goal",
  "baseDefinition": "http://hl7.org/fhir/StructureDefinition/Goal",
  "derivation": "constraint",
  "differential": {
    "element": [
      {
          "id": "Goal",
          "path": "Goal",
          "short": "Goal Profile",
          "definition": "The Goal Profile is based upon the core FHIR Goal Resource"
      },
      {
        "id": "Goal.lifecycleStatus",
        "path": "Goal.lifecycleStatus",
        "mustSupport": true,
        "binding": {
            "strength": "required",
            "description": "Codes that reflect the current state of a goal and whether the goal is still being targeted.",
            "valueSet": "http://hl7.org/fhir/ValueSet/goal-status"
        }
      },
      {
        "id": "Goal.description",
        "path": "Goal.description",
        "short": "Goal Description",
        "mustSupport": true
      },
      {
        "id": "Goal.subject",
        "path": "Goal.subject",
        "short": "Who the goal is intended for",
        "min": 1,
        "max": 1,
        "mustSupport": true,
        "type": [
          {
            "code": "Reference",
            "targetProfile": [
              "http://hl7.org/fhir/ca/core/StructureDefinition/profile-patient"
            ]
          }
        ]
      },
      {
        "id": "Goal.target",
        "path": "Goal.target",
        "short": "Target outcome for the goal",
        "mustSupport": true
      },
      {
        "id": "Goal.target.dueDate",
        "path": "Goal.target.dueDate",
        "short": "Reach goal on or before",
        "mustSupport": true
      }
    ]
  }
}

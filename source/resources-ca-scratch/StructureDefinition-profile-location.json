                "max": "1",
                "type": [
                    {
                        "code": "uri"
                    }
                ],
                "mustSupport": true
            },
            {
                "id": "Location.identifier:LN.value",
                "path": "Location.identifier.value",
                "min": 1,
                "max": "1",
                "type": [
                    {
                        "code": "string"
                    }
                ],
                "mustSupport": true,
                "isModifier": false
            },
            {
                "id": "Location.identifier:OTH",

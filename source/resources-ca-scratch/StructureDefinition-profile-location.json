                "id": "Location.identifier:OTH.value",
                "path": "Location.identifier.value",
                "min": 1,
                "max": "1",
                "type": [
                    {
                        "code": "string"
                    }
                ],
                "mustSupport": true,
                "isModifier": false
            },
            {
                "id": "Location.status",
                "path": "Location.status",

            {
                "id": "Location.name",
                "path": "Location.name",
                "min": 1,
                "mustSupport": true
            },
            {
                "id": "Location.type",
                "path": "Location.type",
                "short": "Location Type",
                "min": 1,
                "mustSupport": true
            },
            {
                "id": "Location.telecom",
                "path": "Location.telecom",
                "mustSupport": true
            },
            {
                "id": "Location.address",
                "path": "Location.address",
                "short": "Physical location",
                "mustSupport": true
            },

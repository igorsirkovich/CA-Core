{
  "resourceType": "SearchParameter",
  "id": "us-core-ethnicity",
  "url": "http://hl7.org/fhir/us/core-r4/SearchParameter/us-core-ethnicity",
  "name": "USCoreEthnicity",
  "status": "active",
  "date": "2016-12-22",
  "publisher": "US Realm Steering Committee",
  "description": "Returns patients with an ethnicity extension matching the specified code.",
  "code": "ethnicity",
  "base": [
    "Patient"
  ],
  "type": "token",
  "expression": "Patient.extension.where(url = 'http://hl7.org/fhir/us/core-r4/StructureDefinition/us-core-ethnicity').extension.value.code",
  "xpath": "f:Patient/f:extension[@url='http://hl7.org/fhir/us/core-r4/StructureDefinition/us-core-ethnicity']/f:extension/f:valueCoding/f:code/@value",
  "xpathUsage": "normal"
}
